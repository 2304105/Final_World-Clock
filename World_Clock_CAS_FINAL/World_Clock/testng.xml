<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<listeners>
<listener class-name="Utilities.Extent_Report"/>
</listeners>
<test thread-count="5" name="Test">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="TestCases.User_Validation"/> 
      <class name="TestCases.World_Clock_Title"/>
      <class name="TestCases.Banglore_info"/>
      <class name="TestCases.London_info"/>
      <class name="TestCases.NewYork_info"/>
      <class name="TestCases.All_Apps"/>
      <class name="TestCases.App_Alphabet_List"/>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="edge">
  <parameter name="browser" value="edge"></parameter>
    <classes>
      <class name="TestCases.User_Validation"/> 
      <class name="TestCases.World_Clock_Title"/>
      <class name="TestCases.Banglore_info"/>
      <class name="TestCases.London_info"/>
      <class name="TestCases.NewYork_info"/>
      <class name="TestCases.All_Apps"/>
      <class name="TestCases.App_Alphabet_List"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
